{
    "version": 2,
    "cmakeMinimumRequired": {
        "major": 3,
        "minor": 20
    },
    "configurePresets": [
        {
            "name": "build_nrf54l15_stdz1",
            "displayName": "Build for nRF54l15 DK nRF54l15 Application",
            "generator": "Ninja",
            "binaryDir": "${sourceDir}/build_nrf54l15_stdz1",
            "cacheVariables": {
                "NCS_TOOLCHAIN_VERSION": "NONE",
                "BOARD": "nrf54l15dk/nrf54l15/cpuapp",
                "SNIPPET": "bt-ll-sw-split",
                "FILE_SUFFIX": "stdz1",
                "BOARD_ROOT": "${sourceDir}/;c:/ncs/ws-2.9.1/subrating"
            }
        }
    ]
}